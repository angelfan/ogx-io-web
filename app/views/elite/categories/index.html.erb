<p id="notice"><%= notice %></p>

<h1>Listing Elite Categories</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elite_categories.each do |elite_category| %>
      <tr>
        <td><%= link_to 'Show', elite_category %></td>
        <td><%= link_to 'Edit', edit_elite_category_path(elite_category) %></td>
        <td><%= link_to 'Destroy', elite_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Category', new_elite_category_path %>
