<p id="notice"><%= notice %></p>

<h1>Listing Admin Elite Categories</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_elite_categories.each do |admin_elite_category| %>
      <tr>
        <td><%= link_to 'Show', admin_elite_category %></td>
        <td><%= link_to 'Edit', edit_admin_elite_category_path(admin_elite_category) %></td>
        <td><%= link_to 'Destroy', admin_elite_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Category', new_admin_elite_category_path %>
